{
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "functions": {
        "api/**": {
            "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
        }
    }
}
